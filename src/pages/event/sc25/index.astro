    <script>
        document.addEventListener('DOMContentLoaded', () => {
            if (window.lucide) window.lucide.createIcons();

            // Fade in Spline iframe when loaded
            const splineIframe = document.getElementById('aura-spline');
            const splineContainer = document.querySelector('.spline-container');

            if (splineIframe && splineContainer) {
                splineIframe.addEventListener('load', () => {
                    splineContainer.classList.remove('opacity-0');
                    splineContainer.classList.add('opacity-100');
                });

                // Fallback: fade in after 3 seconds if load event doesn't fire
                setTimeout(() => {
                    if (splineContainer.classList.contains('opacity-0')) {
                        splineContainer.classList.remove('opacity-0');
                        splineContainer.classList.add('opacity-100');
                    }
                }, 3000);
            }
        });
    </script>

