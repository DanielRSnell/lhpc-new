---
export const prerender = true;
import { SITE_TITLE, SITE_DESCRIPTION } from '../consts';

import { Cta19 } from '@/components/cta19';
import { Pricing36 } from '@/components/pricing36';
import DefaultLayout from '@/layouts/DefaultLayout.astro';
---

<DefaultLayout
  title="Pricing - LaunchHPC AI/HPC Orchestration Platform"
  description="Enterprise-grade AI/HPC orchestration pricing tailored to your infrastructure needs. Scale your compute workloads with flexible, transparent pricing."
>
  <section class="section-padding bg-background">
    <div class="container space-y-6 text-center">
      <div class="mx-auto max-w-3xl">
        <h1 class="mb-4 text-5xl font-bold tracking-tight md:text-6xl">
          Enterprise-grade <span class="text-gradient"
            >AI/HPC orchestration</span
          >
        </h1>
        <p class="text-muted-foreground text-lg md:text-xl">
          Custom pricing tailored to your infrastructure needs. Scale your AI
          and high-performance computing workloads with enterprise-grade
          security and support.
        </p>
      </div>
    </div>
  </section>
  <Pricing36 client:load />
  <Cta19 client:load />
</DefaultLayout>
