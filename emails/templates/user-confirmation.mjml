        <mj-text font-size="11px" font-weight="600" color="#6b7280" text-transform="uppercase" letter-spacing="0.5px" padding="0 0 4px 0">
          Budget
        </mj-text>
        <mj-text font-family="'Fira Code', monospace" font-size="14px" color="#d4d4d4" padding="0 0 16px 0">
          {{ $json.budget }}
        </mj-text>

        <mj-text font-size="11px" font-weight="600" color="#6b7280" text-transform="uppercase" letter-spacing="0.5px" padding="0 0 4px 0">
          Timeline
        </mj-text>
        <mj-text font-family="'Fira Code', monospace" font-size="14px" color="#d4d4d4" padding="0">
          {{ $json.urgency }}
        </mj-text>
      </mj-column>
    </mj-section>

    <!-- Next Steps -->
    <mj-section background-color="#000000" padding="40px 20px 20px 20px">
      <mj-column>
        <mj-text font-family="'Fira Code', monospace" font-size="13px" color="#5cc040" padding="0 0 20px 0" text-transform="uppercase" letter-spacing="1px">
          // Next Steps
        </mj-text>
      </mj-column>
    </mj-section>

    <mj-section background-color="#000000" padding="0 20px 12px 20px">
      <mj-column>
        <mj-text font-size="14px" color="#a3a3a3" padding="0" line-height="1.6">
