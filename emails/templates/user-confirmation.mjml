        <mj-text font-family="'Fira Code', monospace" font-size="14px" color="#d4d4d4" padding="0 0 4px 0">
          {{ $json.email }}
        </mj-text>
        <mj-text font-family="'Fira Code', monospace" font-size="14px" color="#d4d4d4" padding="0 0 16px 0">
