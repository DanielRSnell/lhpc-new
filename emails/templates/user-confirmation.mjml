    <!-- Footer -->
    <mj-section background-color="#000000" padding="40px 20px 32px 20px">
      <mj-column>
        <mj-image src="https://amalgamy.ai/branding/dark.png" alt="Amalgamy" width="100px" align="center" padding="0 0 16px 0" />
        <mj-text font-size="13px" color="#6b7280" align="center" padding="0 0 6px 0">
